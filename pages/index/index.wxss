  width: 50rpx;
  height: 50rpx;
}
.footer-nav view{
  font-size: 26rpx;
  line-height: 40rpx;
}


.loading{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: #7FA0FF; 
  opacity: 1;
}
 .loading.on{
  opacity: 0;
  transform: scale(0.001);
  animation: runs .3s linear;
}
@keyframes runs{
  0%{transform: scale(1);opacity: 1;}
  99%{transform: scale(1);opacity: 0;}
  100%{transform: scale(0.001);opacity: 0;}
} 
